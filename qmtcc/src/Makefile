dist: 
	@mkdir -p .obj/qmtcc && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/qmtcc/ && ( cd `dirname .obj/qmtcc` && $(TAR) qmtcc.tar qmtcc && $(GZIP) qmtcc.tar ) && $(MOVE) `dirname .obj/qmtcc`/qmtcc.tar.gz . && $(DEL_FILE) -r .obj/qmtcc

